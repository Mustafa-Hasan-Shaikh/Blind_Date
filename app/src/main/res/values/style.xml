<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="drawerLayout">#AEADAD</color>
    <style name="InputBox">
        <item name="android:layout_marginTop">10dp</item>
        <item name="boxBackgroundMode">outline</item>
        <item name="boxCornerRadiusBottomStart">15dp</item>
        <item name="boxCornerRadiusBottomEnd">15dp</item>
        <item name="boxCornerRadiusTopStart">15dp</item>
        <item name="boxCornerRadiusTopEnd">15dp</item>
        <item name="hintTextColor">#656464</item>
        <item name="boxStrokeColor">#656464</item>
        <item name="boxStrokeWidth">2dp</item>
        <item name="boxStrokeWidthFocused">2dp</item>
    </style>
    <style name="Heading">
        <item name="android:textSize">36sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="Description">
        <item name="android:textColor">#393939</item>
        <item name="android:textSize">24sp</item>
    </style>

    <style name="Log_button">
        <item name="android:textColor">#323232</item>
        <item name="android:background">@drawable/button</item>
    </style>
</resources>